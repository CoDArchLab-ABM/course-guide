	author = {Edmonds, Bruce and Le Page, Christophe and Bithell, Mike and Chattoe-Brown, Edmund and Grimm, Volker and Meyer, Ruth and Montañola-Sales, Cristina and Ormerod, Paul and Root, Hilton and Squazzoni, Flaminio},
	month = jun,
	year = {2019},
	note = {Publisher: University of Surrey},
	keywords = {Models, Theory, Analogy, Explanation, Prediction, Purpose},
	file = {Different Modelling Purposes:E\:\\Zotero\\storage\\2NBWGDEQ\\6.html:text/html;Edmonds et al_2019_Different Modelling Purposes.pdf:E\:\\OneDrive\\Bib\\Zotero\\Edmonds et al_2019_Different Modelling Purposes.pdf:application/pdf},
}

