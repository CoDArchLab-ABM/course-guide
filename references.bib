	month = dec,
	year = {2017},
	pmid = {29208733},
	note = {Publisher: National Academy of Sciences},
	keywords = {ABM, simulation, modelling},
	pages = {12841--12844},
	file = {Rogers_Cegielski_2017_Opinion.pdf:E\:\\OneDrive\\Bib\\Zotero\\Rogers_Cegielski_2017_Opinion.pdf:application/pdf},
}

