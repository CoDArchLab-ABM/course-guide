	pages = {1--16},
	file = {Bergin_Pardo-Gordo_2021_An Introduction to Simulating Transitions to Agriculture in Prehistory.pdf:E\:\\OneDrive\\Bib\\Zotero\\Bergin_Pardo-Gordo_2021_An Introduction to Simulating Transitions to Agriculture in Prehistory2.pdf:application/pdf;Springer Full Text PDF:E\:\\Zotero\\storage\\ZUWFIRPZ\\Bergin and Pardo-Gordó - 2021 - An Introduction to Simulating Transitions to Agric.pdf:application/pdf},
