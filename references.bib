	keywords = {causal inference, causality, potential outcome, social inquiry, statistical inference, vertical and horizontal view},
	pages = {9--24},
	file = {Snapshot:E\:\\Zotero\\storage\\BDHQQB8V\\9781119704492.html:text/html},
}

@incollection{manzo_agent-based_2022,
	title = {Agent-based {Models} and the {Vertical} {View} on {Mechanism}},
	isbn = {978-1-119-70449-2},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781119704492.ch2},
	abstract = {This chapter explains why an agent-based model (ABM) deep technical infrastructure in fact is responsible for an ABM's high flexibility, granularity, and generality, and thus makes it a powerful tool for the implementation of a vertical view of mechanisms. It focuses on an ABM's intrinsic technical potentialities. Within ABMs, the amount, and the type, of heterogeneity to be assumed becomes a modeling problem itself, and the implications at a higher level of analysis of the choices made at a given level of analysis in terms of heterogeneity can be deductively studied in a systematic way. Contrary to the association that some—usually unfamiliar with the field—tend to establish between ABMs and rational-choice theorizing, the tool is entirely agnostic on the kind of micro-foundations a modeler should subscribe to. Among statistically minded social scientists, those who exploited object-oriented programming to create new statistical methods and/or software packages also tend to recognize an ABM's interest for causal inference.},
