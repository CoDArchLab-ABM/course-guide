@article{Angourakis2017a,
	title = {The {Nice} {Musical} {Chairs} {Model}: {Exploring} the {Role} of {Competition} and {Cooperation} {Between} {Farming} and {Herding} in the {Formation} of {Land} {Use} {Patterns} in {Arid} {Afro}-{Eurasia}},
	volume = {24},
	copyright = {Creative Commons Attribution-ShareAlike 4.0 International Licence (CC-BY-SA)},
	issn = {1072-5369},
	url = {http://dx.doi.org/10.1007/s10816-016-9309-8},
	doi = {10.1007/s10816-016-9309-8},
	abstract = {© 2017, Springer Science+Business Media New York. Following a strictly theory-building approach, we developed an agent-based simulation model, the Nice Musical Chairs model, to represent the competition between groups of stakeholders of farming and herding activities in the arid Afro-Eurasia. The model dee pens the questions raised by the results of our former model, the Musical Chairs model, and further introduces three socio-economic mechanisms, which modulate the behavior and performance of stakeholders and their groups. First, we define land use pairing as the awarding, regarding productivity, of any direct cooperation between farming and herding within a group. Second, group management is modeled as the prerogative of a group leadership to manage stakeholders to pursue a particular proportion between farming and herding. Third, we introduce restricted access to pasture as the engagement in territorial control of rangelands in opposition to an open access regime. An exhaustive exploration of scenarios and parameters placed the control over rangelands as the most significant factor in the formation of land use patterns, followed by land use management. While the effect of land use pairing is mild in comparison, it is still a significant factor in group selection and thus in the persistence of particular land use patterns in the long run.},
	number = {4},
	urldate = {2017-10-15},
	journal = {Journal of Archaeological Method and Theory},
	author = {Angourakis, Andreas and Salpeteur, Matthieu and Martínez Ferreras, Verònica and Gurt Esparraguera, Josep Maria and Ferreras, Verònica Martínez and Gurt Esparraguera, Josep Maria},
	month = sep,
	year = {2017},
	note = {Publisher: Springer US},
	keywords = {Agent-based model, CS5, NMC, Pastoral systems, Land use, Cooperation, Competition, Herder-farmer relationship},
	pages = {1177--1202},
	file = {Angourakis et al_2017_The Nice Musical Chairs Model.pdf:E\:\\OneDrive\\Bib\\Zotero\\Angourakis et al_2017_The Nice Musical Chairs Model.pdf:application/pdf;Angourakis et al_2017_The Nice Musical Chairs Model.pdf:E\:\\OneDrive\\Bib\\Zotero\\Angourakis et al_2017_The Nice Musical Chairs Model2.pdf:application/pdf;Angourakis et al_2017_The Nice Musical Chairs Model.pdf:E\:\\OneDrive\\Bib\\Zotero\\Angourakis et al_2017_The Nice Musical Chairs Model3.pdf:application/pdf;Angourakis et al. - 2017 - The Nice Musical Chairs Model Exploring the Role .pdf:E\:\\Zotero\\storage\\CBVLLY2P\\Angourakis et al. - 2017 - The Nice Musical Chairs Model Exploring the Role .pdf:application/pdf},
