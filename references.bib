\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781119704492.ch2},
	keywords = {agent-based model, micro-foundations, heterogeneity, object-oriented programming, statistical methods, vertical view},
	pages = {25--32},
	file = {Snapshot:E\:\\Zotero\\storage\\SU4UKYU7\\9781119704492.html:text/html},
}

@incollection{manzo_diversity_2022-1,
