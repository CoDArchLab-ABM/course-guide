	volume = {11},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	issn = {2076-3417},
	shorttitle = {Photogrammetry ({SfM}) vs. {Terrestrial} {Laser} {Scanning} ({TLS}) for {Archaeological} {Excavations}},
