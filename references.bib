}

@article{junge_redundancy_2021,
	title = {Redundancy, {Noisy} {Signaling}, and {Semantic} inflation: {Studying} the {Patterns} of {Self}-{Destruction} in {Complex} {Systems}},
	volume = {38},
	issn = {1099-1743},
	shorttitle = {Redundancy, {Noisy} {Signaling}, and {Semantic} inflation},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/sres.2729},
	doi = {10.1002/sres.2729},
	abstract = {Across the natural and social world, some fairly advanced and apparently robust systems destroy themselves without discernible reason, function, or purpose. Whereas such and similar cases have been discussed within specific branches of scholarships, the attempts to propose and test a general theory of self-destruction in complex systems remain, to our knowledge, a thing of the future. To move in this direction, we suggest that the conflicts between explicit stratification, thorough functional differentiation and tacit segmentation within complex systems, are likely to provoke their unintended self-annihilation. Furthermore, we hypothesize that whenever such systems collapse, it is the circular co-dependency of noisy signaling, redundancy, and semantic inflation that sets self-destructive mechanisms in motion. We illustrate our preliminary findings with examples from social, natural, and artificial life; reexamine autoimmunity as a popular model of systemic self-destruction; and indicate potential avenues of empirical research aimed at supporting, qualifying or disproving the underlying hypothesis.},
	language = {en},
	number = {6},
	urldate = {2022-04-11},
	journal = {Systems Research and Behavioral Science},
	author = {Junge, Kay and Postoutenko, Kirill},
	year = {2021},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/sres.2729},
	keywords = {complex systems, autoimmunity, noisy signaling, redundancy, semantic inflation},
	pages = {902--910},
	file = {Junge_Postoutenko_2021_Redundancy, Noisy Signaling, and Semantic inflation.pdf:E\:\\OneDrive\\Bib\\Zotero\\Junge_Postoutenko_2021_Redundancy, Noisy Signaling, and Semantic inflation.pdf:application/pdf;Snapshot:E\:\\Zotero\\storage\\8LTCG278\\sres.html:text/html},
}

@incollection{manzo_diversity_2022,
	title = {The {Diversity} of {Views} on {Causality} and {Mechanisms}},
	isbn = {978-1-119-70449-2},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781119704492.ch1},
	abstract = {This chapter examines the concept of causal inference; discusses the concept of causality; and aims to investigate the concept of mechanism. Thus, various forms of regression analysis became the main tools to establish causal claim, and statistical inference was silently equated to causal inference. Goldthorpe observed that the views on causality combine in practice with distinctive methods of social inquiry. The association between specific methods for causal inference and views on causation—an association recently also documented by Kaidesoja—is especially visible in the recent and flourishing literature on the so-called 'potential outcome' approach. Recent sociological scholarship shows that the bone of contention is the opposition between the vertical and horizontal view of mechanisms that was behind the Hauser–Boudon debate. The analysis of the variety of accounts that the concepts of causality and mechanism have received clearly suggests that the different intuitions on the two notions connect in a systematic way.},
